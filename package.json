{
    "name": "react-redux-isomorphic-app",
    "version": "1.0.0",
    "description": "",
    "main": "index.js",
    "scripts": {
        "dev": "npm-run-all --parallel dev:*",
        "dev-build-server": "webpack --config webpack/dev/webpack.server.js",
        "dev:build-client": "webpack --config webpack/dev/webpack.client.js --watch",
        "dev:server": "nodemon --watch server --exec \"npm run dev-build-server && node --inspect server-build/server.js\"",
        "prod": "npm-run-all --parallel prod:*",
        "prod-build": "npm-run-all --parallel prod:build-server prod:build-client",
        "prod:server": "BABEL_ENV=prod NODE_ENV=production node server-build/server.js",
        "prod:build-server": "BABEL_ENV=prod webpack --config webpack/production/webpack.server.js",
        "prod:build-client": "BABEL_ENV=prod webpack --config webpack/production/webpack.client.js"
    },
    "devDependencies": {
        "@babel/cli": "^7.8.4",
        "@babel/core": "^7.9.0",
        "@babel/plugin-proposal-class-properties": "^7.8.3",
        "@babel/plugin-syntax-dynamic-import": "^7.8.3",
        "@babel/plugin-transform-runtime": "^7.9.0",
        "@babel/preset-env": "^7.9.5",
        "@babel/preset-react": "^7.9.4",
        "@loadable/babel-plugin": "5.12.0",
        "@loadable/component": "5.12.0",
        "@loadable/server": "5.12.0",
        "@loadable/webpack-plugin": "5.12.0",
        "axios": "^0.19.2",
        "babel-loader": "^8.1.0",
        "body-parser": "^1.19.0",
        "clean-webpack-plugin": "^3.0.0",
        "cookie-parser": "^1.4.5",
        "copy-webpack-plugin": "^5.1.1",
        "css-loader": "^3.5.2",
        "dotenv": "^8.2.0",
        "dotenv-webpack": "^1.7.0",
        "express": "^4.17.1",
        "express-http-proxy": "^1.6.0",
        "extract-text-webpack-plugin": "^4.0.0-beta.0",
        "file-loader": "^6.0.0",
        "html-loader": "^1.1.0",
        "html-webpack-plugin": "^4.2.0",
        "mini-css-extract-plugin": "^0.9.0",
        "node-sass": "4.13.1",
        "npm-run-all": "^4.1.5",
        "postcss-loader": "^3.0.0",
        "raw-loader": "^4.0.1",
        "react": "^16.13.1",
        "react-dom": "^16.13.1",
        "react-redux": "^7.2.0",
        "react-router-config": "^5.1.1",
        "react-router-dom": "^5.1.2",
        "redux": "^4.0.5",
        "redux-thunk": "^2.3.0",
        "regenerator-runtime": "0.13.5",
        "sass-loader": "^8.0.2",
        "style-loader": "^1.1.4",
        "universal-cookie": "^4.0.3",
        "webpack": "4.42.1",
        "webpack-bundle-analyzer": "^3.7.0",
        "webpack-cli": "3.3.11",
        "webpack-dev-server": "^3.10.3",
        "webpack-merge": "^4.2.2",
        "webpack-node-externals": "^1.7.2"
    },
